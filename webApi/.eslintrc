{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-console": "off",
        "class-methods-use-this": 0,
        "max-len": [
            2,
            150
        ],
        "linebreak-style": 0,
        "indent": [
            2,
            4
        ]
    }
}